{
  "$schema": "../schema/config_schema.json",
  "config_name": "Honkai Star Rail",
  "events": {
    "file_events": {
      "enabled": true
    },
    "hotkeys": {
      "failsafe": "Shift+Backspace",
      "accept_input": "Shift+]"
    }
  },
  "operators": [
    "$broadcaster",
    "thundercookie15"
  ],
  "irc": {
    "host": "irc.chat.twitch.tv",
    "port": 6697,
    "message_interval": 5
  },
  "sessionlog": {
    "enable_chatlog": true,
    "enable_consolelog": true
  },
  "teams": [
    {
      "type": "Default",
      "name": "Chat",
      "queue_length": 10,
      "hidden": false,
      "use_random_inputs": false,
      "channels": [
        "thundercookie15",
        "sayu"
      ],
      "actionset": {
        "type": "HSR_KB",
        "action_prefix": "+",
        "doc_url": "https://gist.github.com/thundercookie15/e90fe1d1eccd4d5089521a3434d8ade6",
        "player_index": 0,
        "macro_file": "data/macros/honkaistarrail.json",
        "persistent_macros": true,
        "allow_changing_macros": true,
        "input_server": {
          "type": "remote",
          "port": 33001
        }
      }
    }
  ]
}